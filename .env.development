# Variables de Supabase para desarrollo
NEXT_PUBLIC_SUPABASE_URL=tu_url_de_supabase_para_desarrollo
NEXT_PUBLIC_SUPABASE_ANON_KEY=tu_clave_anonima_para_desarrollo
SUPABASE_SERVICE_ROLE_KEY=tu_clave_de_servicio_para_desarrollo
