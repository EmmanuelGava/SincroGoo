# Modo desarrollo sin autenticaci√≥n
DEV_MODE_NO_AUTH=true

# Variables de NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=tu_secreto_para_desarrollo

# Credenciales de Google OAuth (usar las mismas que en .env.local o placeholders para desarrollo)
GOOGLE_CLIENT_ID=tu_client_id_para_desarrollo
